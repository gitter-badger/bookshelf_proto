{
    "$shema": "http//json-schema.org/draft-04/schema#",
    "id": "bookMaster-schema.json",
    "title": "書籍マスタ",
    "type": "object",
    "properties": {
        "bookId": {
            "title": "書籍ID",
            "type": "integer",
            "required": true
        },
        "name": {
            "title": "書籍名",
            "type": "string",
            "maxlength": 100,
            "required": true
        },
        "isbnCode": {
            "title": "ISBNコード",
            "type": "string",
            "maxlength": 20,
            "required": true
        },
        "author": {
            "title": "著者",
            "type": "string",
            "maxlength": 100,
            "required": true
        },
        "publisher": {
            "title": "出版社",
            "type": "string",
            "maxlength": 40,
            "required": true
        },
        "publishDate": {
            "title": "発行日",
            "type": "string",
            "format": "date"
        },
        "keyword1": {
            "title": "キーワード1",
            "type": "string",
            "maxlength": 20
        },
        "keyword2": {
            "title": "キーワード2",
            "type": "string",
            "maxlength": 20
        },
        "keyword3": {
            "title": "キーワード3",
            "type": "string",
            "maxlength": 20
        },
        "createdTime": {
            "title": "作成日時",
            "type": "string",
            "format": "date-time"
        },
        "createdId": {
            "title": "作成者ID",
            "type": "integer"
        },
        "updatedTime": {
            "title": "作成日時",
            "type": "string",
            "format": "date-time"
        },
        "updatedId": {
            "title": "更新者ID",
            "type": "integer"
        }
    }
}
