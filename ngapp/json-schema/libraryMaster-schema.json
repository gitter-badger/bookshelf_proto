{
    "$shema": "http//json-schema.org/draft-04/schema#",
    "id": "libraryMaster-schema.json",
    "title": "蔵書マスタ",
    "type": "object",
    "properties": {
        "libraryId": {
            "title": "蔵書ID",
            "type": "integer",
            "required": true
        },
        "bookId": {
            "title": "書籍ID",
            "type": "integer",
            "required": true
        },
        "providerId": {
            "title": "提供者ID",
            "type": "integer",
            "required": true
        },
        "state": {
            "title": "状態",
            "type": "integer"
        },
        "borrowerId": {
            "title": "借用者ID",
            "type": "integer"
        },
        "createdTime": {
            "title": "作成日時",
            "type": "string",
            "format": "date-time"
        },
        "createdId": {
            "title": "作成者ID",
            "type": "integer"
        },
        "updatedTime": {
            "title": "作成日時",
            "type": "string",
            "format": "date-time"
        },
        "updatedId": {
            "title": "更新者ID",
            "type": "integer"
        }
    }
}
